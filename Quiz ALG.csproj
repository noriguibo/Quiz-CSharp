<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Quiz_ALG</RootNamespace>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>download.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>    
    <None Remove="Resources\Images\background.png" />
    <None Remove="Resources\Images\close.png" />
    <None Remove="Resources\Images\correct.png" />
    <None Remove="Resources\Images\incorrect.png" />
    <None Remove="Resources\Images\minimize.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="download.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Npgsql" Version="6.0.7" />
    <PackageReference Include="System.Data.SQLite" Version="1.0.116" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Images\background.png" />
    <Resource Include="Resources\Images\close.png" />
    <Resource Include="Resources\Images\correct.png" />
    <Resource Include="Resources\Images\incorrect.png" />
    <Resource Include="Resources\Images\minimize.png" />
  </ItemGroup>

</Project>
